<article>
  <div class="row">

     <div class="col-md-2">
     <p>Awesome</p>
  <ul class="nav nav-pills nav-stacked">
   <li role="presentation"><%= link_to "Back to Settings", settings_path %></li>
   </ul>
     </div>

       <div class="col-md-8">
       <article class="panel panel-info">
         <header class="panel-heading">
             <h3>Summary for Tracking no. <%= @checkin.tracking %></h3>
          </header>
          <section class="panel-body">
           <article class="well well-sm">
            <h3>Current Location is <%= @checkin.location %></h3>
            </article>
            <article class="well well-sm">
            <h3>Notice: <small> <%= @checkin.notice %></small> </h3>
           </article>
            <article class="well well-sm">
            <h3>Checked In at: <%= @checkin.time_checked_in.strftime("%I:%M %p %b %d %Y") %></h3>
            </article>
       </section>
          <footer class="panel-footer">
           <p class="small">Check again later</p>
          </footer>
        </article>
      </div>

  </div>
  
</article>
